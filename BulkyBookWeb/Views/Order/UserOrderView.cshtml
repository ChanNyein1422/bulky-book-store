@model List<Data.Models.tbOrder>

<div class="text-center">
    <p>Your Orders</p>
    @if(Model.Count() > 0)
    {
        @foreach(var item in Model)
        {
            <ul class="mb-3">
                <li>Order Code: @item.OrderCode</li>
                <li>Total Books: @item.TotalBooks</li>
                <li>Price Amount: @item.TotalAmount</li>
            </ul>
        }
    }
</div>

